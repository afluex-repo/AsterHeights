    function ChangeUTR() {
        var ActualNeftNumber = $('#TransactionNumber').val();
        var PaymentMode = $('#PaymentMode').val();
        if (PaymentMode == 2 || PaymentMode == 3 || PaymentMode == 5 || PaymentMode == 6) {
            $("#UtrNumber").val(ActualNeftNumber);

            $.ajax({
                url: '/Plot/CheckUTR', type: 'get', dataType: 'json',
                data: { 'UtrNumber': ActualNeftNumber },
                success: function (data) {
                    //console.log(data)
                    $("#divload").css({ 'display': 'none' });
                    if (data.Result == "yes") {
                        // debugger
                        var UTRDetails = data.lstUtr;
                        $("#test-table tbody tr").remove();
                        $("#test-table #test-footer tr").remove();
                        if (UTRDetails.length > 0) {
                            var row = $("#test-table tr:first").clone(true);
                            var html = '';
                            $.each(UTRDetails, function (i, v) {
                                $(row).find('.CustomerId').html(v.CustomerID);
                                $(row).find('.CustomerName').html(v.CustomerName);
                                $(row).find('.PlotNumber').html(v.PlotNumber);
                                $(row).find('.Amount').html(v.UtrPaidAmount);
                                $(row).find('.UtrDate').html(v.UtrDate);
                                $("#test-table").append(row);
                                row = $("#test-table tr:first").clone(true)
                                //console.log(i);
                            });

                            $('#test-table #test-footer').append('<tr><th colspan="4" style="text-align:right">Remaining UTR Amount: </th><th style="text-align:center">' + data.RemainingUtrAmount + '</th></tr>');

                            $("#UtrAmount").val(data.UtrAmount);
                            $("#UtrAmount").addClass('blockTyping');
                            $("#test-table").show();
                            $("#TblAddress").show();
                            //console.log($("#PayAmount").val())
                            //console.log(data.RemainingUtrAmount)
                            if (Number($("#PayAmount").val()) > Number(data.RemainingUtrAmount)) {
                                alert('PayAmount is greater than Remaining UTR Amount.....')
                                $("#PayAmount").val('');
                                $("#PayAmount").focus();
                                $("#TransactionNumber").val('');
                                $("#UtrNumber").val('');
                                $("#UtrAmount").val('');
                                $("#UtrAmount").removeClass('blockTyping');
                            }
                            else {

                            }
                        }
                        else {
                            $("#test-table thead tbody tr").remove();
                            $("#test-table #test-footer tr").remove();
                            $("#test-table").hide();
                        }

                    }
                    else {
                        $("#UtrAmount").val('');
                        $("#UtrAmount").removeClass('blockTyping');
                        $("#PayAmount").val('');
                        $("#test-table").hide();
                        $("#TblAddress").hide();
                        $("#test-table thead tbody tr").remove();
                    }
                }

            });
        }
        else {
            $("#UtrAmount").val('');
            $("#UtrAmount").removeClass('blockTyping');
            $('#UtrNumber').val('');
        }

    }
