@model AsterHeight.Models.Plot

@{
    ViewBag.Title = "EMIPayment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("EMIPayment", "Plot", FormMethod.Post, new { @id = "form1" }))
{



    <section class="section">
        <ul class="breadcrumb breadcrumb-style ">
            <li class="breadcrumb-item">
                <a href="#">
                    <i data-feather="home"></i>
                </a>
            </li>
            <li class="breadcrumb-item">Plot</li>
            <li class="breadcrumb-item">EMI Payment</li>
        </ul>
    </section>


    <div class="section-body">
        <div class="col-12 col-md-12 col-lg-12">
            <div class="card">
                <div class="card-header">
                    <h4>EMI Payment</h4>

                    <p style="color: red">
                        @TempData["Plot"]
                    </p>
                </div>
                <div class="card-body">

                    <div class="row">
                        <div class="col-12 col-md-3 col-lg-3">
                            <div class="form-group">
                                Select Site <span style="color: red">*</span>
                                @Html.DropDownListFor(m => m.SiteID, new SelectList(ViewBag.ddlSite, "Value", "Text"), new { @class = "form-control", @onchange = "return onSiteChange();" })
                            </div>
                        </div>
                        <div class="col-12 col-md-3 col-lg-3">
                            <div class="form-group">
                                Select Phase <span style="color: red">*</span>
                                @Html.DropDownListFor(m => m.SectorID, new SelectList(ViewBag.ddlSector, "Value", "Text"), new { @class = "form-control", @onchange = "return onSectorChange();" })
                            </div>
                        </div>
                        <div class="col-12 col-md-3 col-lg-3">
                            <div class="form-group">
                                Select Block <span style="color: red">*</span>
                                @Html.DropDownListFor(m => m.BlockID, new SelectList(ViewBag.ddlBlock, "Value", "Text"), new { @class = "form-control" })
                            </div>
                        </div>

                        <div class="col-12 col-md-3 col-lg-3">
                            <div class="form-group">
                                Plot Number <span style="color: red">*</span>
                                @Html.TextBoxFor(m => m.PlotNumber, new { @class = "form-control", @Placeholder = "Plot Number", @style = "text-transform:uppercase" })
                                @Html.HiddenFor(m => m.PlotID)
                            </div>
                        </div>
                        <div class="col-12 col-md-3 col-lg-3">
                            <div class="form-group">
                                Booking Number
                                @Html.TextBoxFor(m => m.BookingNumber, new { @class = "form-control", @Placeholder = "Booking Number" })
                                @Html.HiddenFor(m => m.PK_BookingId)
                            </div>
                        </div>
                        <div class="col-12 col-md-3 col-lg-3">
                            <div class="form-group">
                                <br />
                                <input type="button" name="SearchPlot" value="Search" id="btnSearchPlot" class="btn btn-success" onclick="return GetDetails();" />
                            </div>
                        </div>
                    </div>
                    <div class="row" id="Details1" style="display: none;">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div class="x_title">
                                    <h2>Plot Details @*<small>List Of Sites</small>*@</h2>
                                    @*<ul class="nav navbar-right panel_toolbox">
                                           // <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                                        </ul>*@
                                    <div class="clearfix"></div>
                                </div>

                                <div class="row ">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Associate ID :</label>
                                            <label id="AssoID"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Associate Name :</label>
                                            <label id="AssoName"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label> Customer ID :</label>
                                            <label id="CustID"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label> Customer Name :</label>
                                            <label id="CustName"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Plot Rate :</label>
                                            <label id="Plotrte"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Plot Area :</label>
                                            <label id="Plotarea"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Actual Plot Amount :</label>
                                            <label id="ActPlotAmt"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Discount :</label>
                                            <label id="disct"></label>
                                        </div>
                                    </div>

                                </div>
                                <div class="row">

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Booking Amount :</label>
                                            <label id="bookingAmt"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Booking Date :</label>
                                            <label id="Bookigdt"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Payment Plan :</label>
                                            <label id="Payplan"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Total Paid Amount:</label>
                                            <label id="Payamt"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Allotment Date:</label>
                                            <label id="Paydt"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>No. of Installments:</label>
                                            <label id="noOfinst"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Installment Amount :</label>
                                            <label id="instAmt"></label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Balance :</label>
                                            <label id="bal"></label>
                                        </div>
                                    </div>
                                </div>


                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            Pay Amount<span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.PaidAmount, new { @class = "form-control", @Placeholder = "Pay Amount", @onchange = "return validatePayAmount()", @onkeypress = "return  isNumberOrDecimal(event);", @maxlength = "30" })
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            Payment Date<span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.PaymentDate, new { @class = "form-control", @Placeholder = "Select Payment Date" })
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            Payment Mode <span style="color: red">*</span>
                                            @Html.DropDownListFor(m => m.PaymentMode, new SelectList(ViewBag.ddlPaymentMode, "Value", "Text"), new { @class = "form-control", @onchange = "return CheckPayMode()" })
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            Receipt No.<span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.ReceiptNo, new { @class = "form-control", @Placeholder = "Enter Receipt No" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row" id="divPaymentMode" style="display: none">
                                    <div class="col-md-3" id="divtransactiono" style="display: none">
                                        <div class="form-group">
                                            <span id="txnNo"></span> <span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.TransactionNumber, new { @class = "form-control", @Placeholder = "Cheque/Transaction Number", @maxlength = "50", @onchange = "return ChangeUTR();" })
                                        </div>
                                    </div>
                                    <div class="col-md-3" id="divtransationdate" style="display: none">
                                        <div class="form-group">
                                            <span id="txndt"></span><span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.TransactionDate, new { @class = "form-control", @Placeholder = "Cheque/Transaction Date", @Readonly = "readonly" })
                                        </div>
                                    </div>
                                    <div class="col-md-3" id="divbankname" style="display: none">
                                        <div class="form-group">
                                            Bank Name <span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.BankName, new { @class = "form-control", @Placeholder = "Bank Name" })
                                        </div>
                                    </div>
                                    <div class="input-field col-md-3" id="divAssociateId" style="display: none">
                                        <div class="form-group">
                                            <span id="txnAssociateId"></span> <span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.AssociateLoginID, new { @class = "form-control", @Placeholder = "Associate Id", @maxlength = "50", @onchange = "return getAssociateName();" })
                                            @Html.HiddenFor(m => m.AdjustmentId)
                                        </div>
                                    </div>
                                    <div class="input-field col-md-3" id="divAssociateName" style="display: none">
                                        <div class="form-group">
                                            <span id="txnAssociateName"></span> <span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.SponsorName, new { @class = "form-control", @Placeholder = "Associate Name", @maxlength = "50", @readonly = "true" })
                                        </div>
                                    </div>
                                    <div class="col-md-3" id="divbankbranch" style="display: none">
                                        <div class="form-group">
                                            Bank Branch <span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.BankBranch, new { @class = "form-control", @Placeholder = "Bank Branch" })
                                        </div>
                                    </div>
                                    <div class="col-md-3" id="divutrname" style="display: none">
                                        <div class="form-group">
                                            UTR No. <span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.UtrNumber, new { @class = "form-control blockTyping", @Placeholder = "Utr Number" })
                                        </div>
                                    </div>
                                    <div class="col-md-3" id="divutramount" style="display: none">
                                        <div class="form-group">
                                            Utr Amount<span style="color: red">*</span>
                                            @Html.TextBoxFor(m => m.UtrAmount, new { @class = "form-control", @Placeholder = "Utr Amount", onkeypress = "return isNumberOrDecimal(event);" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12" style="padding-right: 24px; padding-left: 24px; display: none" id="TblAddress">
                                    <div class="row">
                                        <div class="table-responsive">
                                            <table id="test-table" class="table table-bordered table-md">
                                                <thead style="text-align:center">
                                                    <tr class="thead">
                                                        <th class="CustomerId">Customer Id</th>
                                                        <th class="CustomerName">Customer Name</th>
                                                        <th class="PlotNumber">PlotNumber</th>
                                                        <th class="UtrDate">Payment Date</th>
                                                        <th class="Amount">Paid Utr Amount</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="test-body" style="padding: 0px 0px;text-align:center"></tbody>
                                                <tfoot id="test-footer"></tfoot>
                                            </table>
                                        </div>

                                    </div>
                                </div>
                                <div class="row ">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <input type="submit" name="Save" value="Save" id="btnSave" class="btn btn-success" onclick="return validateform();" />
                                        </div>
                                    </div>
                                </div>

                                <div class="x_content">
                                    <div class="x_title">
                                        <h2>Installment Details @*<small>List Of Sites</small>*@</h2>
                                        <ul class="nav navbar-right panel_toolbox">
                                            <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                                        </ul>
                                        <div class="clearfix"></div>
                                    </div>
                                    <table id="tblData" width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                        <thead>
                                            <tr>
                                                <th hidden>PK_BookingId</th>
                                                <th hidden>BookingID</th>
                                                @* <th>S No.</th>*@
                                                <th>Installment No.</th>
                                                <th>Installment Date</th>
                                                <th>Installment Amount</th>
                                                <th>Paid Amount</th>
                                                <th>Payment Date</th>
                                                <th>Due Amount</th>
                                                <th>Payment Mode</th>
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>


}



<script src="~/files/bower_components/jquery/js/jquery.min.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
<script>


    $(document).ready(function () {
        $("#divload").css({ 'display': 'none' });
    });

    function validatePayAmount() {
        debugger
        var balanceAmount = document.getElementById("bal").innerHTML;
        var payAmount = $('#PaidAmount').val();

        if ((Number)(payAmount) > (Number)(balanceAmount)) {
            $("#PaidAmount").val('');
            $("#PaidAmount").addClass('errortext');
            alert('Pay Amount cannot be greater than Remaining Amount.\nRemaining Amount is : Rs.' + balanceAmount);
        }
        else {
            $("#PaidAmount").removeClass('errortext');
        }
    }

    function getStateCity() {

        var pincode = $('#Pincode').val();
        $.ajax({
            url: '/Master/GetStateCity', type: 'post', dataType: 'json',
            data: { 'Pincode': pincode },
            success: function (data) {
                if (data.Result == "yes") {
                    $("#State").val(data.State);
                    $("#City").val(data.City);
                    $("#State").removeClass('errortext');
                    $("#City").removeClass('errortext');
                    $("#Pincode").removeClass('errortext');
                }
                else if (data.Result == "no") {
                    $("#State").val('');
                    $("#City").val('');
                    $("#State").addClass('errortext');
                    $("#City").addClass('errortext');
                    $("#Pincode").addClass('errortext');
                    $('#Pincode').focus();
                }
            }

        });
    }

    function isNumberOrDecimal(evt) {
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode != 46 && charCode > 31
          && (charCode < 48 || charCode > 57))
            return false;

        return true;
    }

    function onlyAlphabets(e, t) {
        try {
            if (window.event) {
                var charCode = window.event.keyCode;
            }
            else if (e) {
                var charCode = e.which;
            }
            else { return true; }
            if ((charCode > 64 && charCode < 91) || (charCode > 96 && charCode < 123))
                return true;
            else
                return false;
        }
        catch (err) {
            alert(err.Description);
        }
    }

    function onSiteChange() {
        $("#divload").css({ 'display': '' });
        var value = $('#SiteID').val();
        var values = $('#SectorID').val();
        var tr = [];
        $.ajax({
            url: "/Master/GetSiteDetails",
            Type: "Get",
            DataType: "JSON",
            data: { SiteID: value },
            success: function (data) {
                $("#divload").css({ 'display': 'none' });
                var j = 1;
                if (data.Result == "yes") {
                    $('#Rate').val(data.Rate);
                    $("#SectorID").html(""); // clear before appending new list
                    $("#SectorID").empty();
                    $("#SectorID").append(
                          $('<option></option>').val(0).html('Select Phase'));
                    $.each(data.ddlSector, function (index, Sectorcount) {
                        $("#SectorID").append(
                              $('<option></option>').val(Sectorcount.Value).html(Sectorcount.Text));

                    });

                }
                else {
                    $("#SectorID").empty();
                    $('#Rate').val('');
                }


            }

        });

    }

    function onSectorChange() {
        $("#divload").css({ 'display': '' });
        var siteID = $('#SiteID').val();
        var sectorID = $('#SectorID').val();
        $.ajax({
            url: '/Master/GetBlockList', type: 'post', dataType: 'json',
            data: { 'SiteID': siteID, 'sectorID': sectorID },
            success: function (data) {
                $("#divload").css({ 'display': 'none' });

                $("#BlockID").html(""); // clear before appending new list
                $("#BlockID").empty();
                $("#BlockID").append(
                      $('<option></option>').val(0).html('Select Block'));
                $.each(data.lstBlock, function (index, BlockCount) {
                    $("#BlockID").append(
                          $('<option></option>').val(BlockCount.Value).html(BlockCount.Text));
                });

            }

        });
    }

