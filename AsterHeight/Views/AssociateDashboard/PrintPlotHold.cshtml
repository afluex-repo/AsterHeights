@model AsterHeight.Models.Plot

@{
    ViewBag.Title = "PrintPlotHold";
    Layout = "~/Views/Shared/AssociateLayout.cshtml";
}

<html>
<head>
    <title>PrintPlotHold</title>
    <style>
        table, th, td {
            font-size: medium;
            /*border: 1px solid black;*/
            border-collapse: collapse;
        }

        th, td {
            font-size: medium;
            padding: 5px;
        }
    </style>

</head>
<body>
    <div style="border:3px solid #a20e0e;padding-left:10px;padding-right:10px;">
        <table style="border: 0px none black;text-align:center">
            <tr style="border: 0px none black;">
                <td>

                    <img src="~/AssociatePanelcss/assets/img/moonlitecorporate.png" class="img-fluid" alt="logo1" style="max-width:28%;" />
                    <br /><B>CIN : U74999UP2019PTC116517</B>
                </td>
                <td style="border: 0px none black;"></td>


                <td style="width: 52%">
                    <img src="~/Web/assets/images/logo_ori.png" class="img-fluid" alt="logo1" style="max-width: 15%;" />
                    <br /><B>CIN : U70200UP2019PTC116042</B>
                </td>
            </tr>

        </table>
        <br />

        <table style="width:100%;background:#a20e0e;color:white;">
            <tr>
                <td style="text-align:center">
                    Office Copy
                </td>

            </tr>

        </table>
        <br />
        <table style="width:100%;background:#f6f6f6;color:#202021;">
            <tr>
                <th style="text-align:center">
                    <h3>Plot Hold Receipt</h3>
                </th>

            </tr>
        </table>
        <table style="width: 100%;" class="table table-striped">

            <tr>
                <td style="border: 1px solid black;"><b>Hold For:    </b>@ViewBag.SiteName</td>
                <td colspan="2" style="border: 1px solid black;"><b>Date :    </b>@DateTime.Now.ToString("dd/MM/yyyy")</td>
                <td style="border: 1px solid black;"><b>Receipt No.:   </b>H-@ViewBag.PK_PlotHoldID</td>
            </tr>
        </table>
        <br />
        <table style="width: 100%;" class="table table-striped">

            <tr>
                <td style="border: 1px solid black;" colspan="3"><b>Name :    </b>@ViewBag.CustomerName</td>
                <td style="border: 1px solid black;" colspan="3"><b>Mobile :    </b>@ViewBag.Mobile</td>
            </tr>

        </table>
        <br />
        <table style="width: 100%;" class="table table-striped">
            <tr>

                <td style="border: 1px solid black;" colspan="2"><b> Site:    </b>@ViewBag.SiteName</td>
                <td style="border: 1px solid black;"><b>Sector:    </b>@ViewBag.SectorName</td>
                <td style="border: 1px solid black;"><b>Block:    </b>@ViewBag.BlockName</td>
                <td style="border: 1px solid black;"><b> Plot No:    </b>@ViewBag.PlotNo</td>
            </tr>

            <tr>
                <td style="border: 1px solid black;" colspan="2"><b>Hold Amount:     </b>@ViewBag.HoldAmount /-</td>
                <td style="border: 1px solid black;" colspan="2"><b>In Words:     </b>@ViewBag.Amountwords</td>
                <td style="border: 1px solid black;"><b>Area:    </b>@ViewBag.PlotArea</td>


            </tr>



            <tr>
                <td style="border: 1px solid black;" colspan="5"><b>Remark:     </b>@ViewBag.Remark</td>

            </tr>

        </table>

        <br />

        <br />
        <br />


        <table style="width: 100%;" class="table table-striped">
            @*<tr>
                    <td style="text-align:left;" colspan="2"><b>  Note :-   </b></td>

                </tr>
                <tr>
                    <td style="text-align:left;" colspan="2">1.Booking will be valid only for three months.</td>

                </tr>
                <tr>
                    <td style="text-align:left;" colspan="2">2.Booking receipt will be valid only till Registry.</td>

                </tr>
                <tr>
                    <td style="text-align:left;" colspan="2"> 3.Possession will be given after Registry.</td>

                </tr>*@
            <tr>
                @*<td style="text-align:left;"> 4.Receipt valid subject to realisation of cheque.</td>*@
                <td style="text-align:right;margin-right:100px;"><b style="margin-right: 40px;">Director Sign    </b></td>
            </tr>
        </table>

        <br />

        <table style="width: 100%;text-align:center">


            @*<td><b>Contact no-</b>@ViewBag.ContactNo</td>*@

            <tr>
                <td>

                    <b>LandLine no-</b>@ViewBag.LandLine

                </td>
                <td>
                    <b>Website:</b>   @ViewBag.Website

                </td>
                <td><b>Email : </b>@ViewBag.EmailID</td>
            </tr>
        </table>
        <br />
        <table style="width:100%;background:#a20e0e;color:white;">
            <tr>
                <td style="text-align:center">
                    @ViewBag.CompanyAddress
                </td>
            </tr>
        </table>
        <br />
        <br />

    </div>


